    "axios": "^1",
    "dayjs": "^1.11.13",
    "exceljs": "^4.4.0",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^3.8.4",
    "lodash": "^4.17.21",
    "video.js": "8.21.0",
    "videojs-youtube": "^3.0.1",
    "vue-router": "^4.0.3",
    "vue3-cookies": "^1.0.6",
    "vue3-toastify": "^0.2.3",
    "vue3-youtube": "^0.1.9",
